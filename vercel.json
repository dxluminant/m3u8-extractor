{
    "version": 2,
    "builds": [
        {
            "src": "api/extract_m3u8.py",
            "use": "@vercel/python"
        },
        {
            "src": "public/**",
            "use": "@vercel/static"
        }
    ],
    "routes": [
        {
            "src": "/api/(.*)",
            "dest": "api/extract_m3u8.py"
        },
        {
            "src": "/(.*)",
            "dest": "public/$1"
        }
    ],
    "env": {
        "PLAYWRIGHT_BROWSERS_PATH": "0"
    }
}
